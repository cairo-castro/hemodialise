        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "cypress": "^13.5.0",
        "eslint": "^8.35.0",
        "eslint-plugin-vue": "^9.9.0",
        "jsdom": "^22.1.0",
        "laravel-vite-plugin": "^1.0.0",
        "tailwindcss": "^4.0.0",
        "terser": "^5.4.0",
        "typescript": "~5.6.2",
        "vite": "^5.2.0",
